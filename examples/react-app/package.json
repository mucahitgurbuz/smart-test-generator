{
  "name": "smart-test-gen-react-example",
  "version": "1.0.0",
  "description": "React example for Smart Test Generator demonstration",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
